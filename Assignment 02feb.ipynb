   ],
   "source": [
    "# Using a while loop to calculate sum and product\n",
    "count = 1\n",
    "sum_result = 0\n",
    "product_result = 1\n",
    "\n",
    "while count <= 10:\n",
    "    sum_result += count\n",
    "    product_result *= count\n",
    "    count += 1\n",
    "\n",
    "print(\"Sum using while loop:\", sum_result)\n",
    "print(\"Product using while loop:\", product_result)\n"
   ]
